// Returns the last element of the queue
id_t_ list_[N_+1];
int[0,N_] len_;
void enqueue_(id_t_ element_)
{
        list_[len_++] = element_;
}
void dequeue_()
{
        int i_ = 0;
        len_ -= 1;
        while (i_ &lt; len_)
        {
                list_[i_] = list_[i_ + 1];
                i_++;
        }
        list_[i_] = 0;
}
id_t_ front_()
{
   return list_[0];
}
id_t_ tail_()
{
   return list_[len_ - 1];
}</declaration>
